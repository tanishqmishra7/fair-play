<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".First_Page">


    <ScrollView
        android:id="@+id/sc1"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="0dp"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="@+id/bottomNavigationView"
        app:layout_constraintTop_toBottomOf="@+id/imageView29"
        tools:ignore="SpeakableTextPresentCheck">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#FEDEBE"
            tools:context=".MainActivity">

            <ImageView
                android:id="@+id/imageView29"
                android:layout_width="match_parent"
                android:layout_height="110dp"
                android:layout_marginTop="0dp"
                app:layout_constraintBottom_toTopOf="@+id/game1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/header" />

            <ImageView
                android:id="@+id/game1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:clickable="true"
                android:focusable="true"
                android:onClick="gm1"
                app:layout_constraintBottom_toTopOf="@+id/game2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView29"
                app:srcCompat="@drawable/mq"
                tools:ignore="SpeakableTextPresentCheck" />

            <ImageView
                android:id="@+id/game2"
                android:layout_width="370dp"
                android:layout_height="225dp"
                android:layout_marginTop="50dp"
                android:clickable="true"
                android:focusable="true"
                android:onClick="gm2"
                android:visibility="visible"
                app:layout_constraintBottom_toTopOf="@+id/game3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/game1"
                app:srcCompat="@drawable/ttt"
                tools:ignore="SpeakableTextPresentCheck" />

            <ImageView
                android:id="@+id/game3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                android:layout_marginBottom="100dp"
                android:clickable="true"
                android:focusable="true"
                android:onClick="gm3"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/game2"
                app:srcCompat="@drawable/m"
                tools:ignore="SpeakableTextPresentCheck,ImageContrastCheck" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="150dp"
                android:fontFamily="@font/amarante"
                android:text="Maths Quiz"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="#000000"
                android:textSize="40dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/game1"
                app:layout_constraintEnd_toEndOf="@+id/game1"
                app:layout_constraintStart_toStartOf="@+id/game1"
                app:layout_constraintTop_toTopOf="@+id/game1" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="140dp"
                android:layout_marginLeft="140dp"
                android:fontFamily="@font/amarante"
                android:text="Tic Tac Toe"
                android:textColor="#000000"
                android:textSize="40dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/game2"
                app:layout_constraintEnd_toEndOf="@+id/game2"
                app:layout_constraintStart_toStartOf="@+id/game2"
                app:layout_constraintTop_toTopOf="@+id/game2" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="140dp"
                android:layout_marginRight="140dp"
                android:fontFamily="@font/amarante"
                android:text="Maze World"
                android:textColor="#000000"
                android:textSize="40dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/game3"
                app:layout_constraintEnd_toEndOf="@+id/game3"
                app:layout_constraintStart_toStartOf="@+id/game3"
                app:layout_constraintTop_toTopOf="@+id/game3" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</FrameLayout>